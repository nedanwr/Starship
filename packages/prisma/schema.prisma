// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AllowedGuilds {
    id       BigInt @id @unique
    name     String
    icon     String?
    owner_id BigInt

    @@map(name: "allowed_guilds")
}
